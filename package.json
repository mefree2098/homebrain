{
  "name": "HomeBrain",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "setup": "npm run client-install && npm run server-install",
    "postinstall": "echo \"postinstall disabled; run npm run setup\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd client && npm run dev",
    "client-install": "cd client && npm install",
    "server": "cd server && npm run dev",
    "server-install": "cd server && npm install",
    "start": "concurrently -n \"client,server,insteon\" \"npm run client\" \"npm run server\" \"npm run insteon:start\"",
    "debug": "concurrently -n \"client,server\" \"npm run client\" \"cross-env NODE_OPTIONS=--inspect-brk=9229 npm run server\"",
    "seed": "cd server && node scripts/seedDatabase.js",
    "seed:devices": "cd server && node scripts/seedDatabase.js devices",
    "seed:scenes": "cd server && node scripts/seedDatabase.js scenes",
    "seed:automations": "cd server && node scripts/seedDatabase.js automations",
    "seed:voice": "cd server && node scripts/seedDatabase.js voiceDevices",
    "seed:profiles": "cd server && node scripts/seedDatabase.js userProfiles",
    "seed:force": "cd server && node scripts/seedDatabase.js --force",
    "clean": "cd server && node scripts/cleanDatabase.js",
    "clean:confirm": "cd server && node scripts/cleanDatabase.js --confirm",
    "reset": "npm run clean:confirm && npm run seed",
    "insteon:start": "node scripts/startInsteonService.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "concurrently": "^9.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
